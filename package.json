{
  "name": "deal-pulse",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "concurrently \"tsx watch src/server.ts\" \"vite --config web/vite.config.ts\"",
    "build": "tsc -p . && vite build --config web/vite.config.ts",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "express": "^4.19.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.7.0",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.8",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^9.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.5.0",
    "vite": "^5.2.0"
  }
}